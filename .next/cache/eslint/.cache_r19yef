;\r\nimport { UilBill, UilBox } from '@iconscout/react-unicons';\r\nimport Cooking from '../../assets/cooking.png';\r\nimport Onway from '../../assets/onway.png';\r\nimport Spinner from '../../assets/spinner.svg';\r\nimport Image from \"next/image\";\r\nimport { useEffect } from \"react\";\r\n\r\nexport const getServerSideProps = async (props) => {\r\n    const query = `*[_type == 'order' && _id == '${props.params.id}']`;\r\n    const order = await client.fetch(query);\r\n\r\n    return {\r\n        props: {\r\n            order: order[0]\r\n        }\r\n    }\r\n}\r\n\r\nexport default function Orders({order}) {\r\n\r\n    useEffect(() => {\r\n        if (order.status > 3) {\r\n            localStorage.clear();\r\n        }\r\n    }, [order]);\r\n\r\n    const displayPizzaSize = (s) => {\r\n        switch (s) {\r\n            case 0:\r\n                return 'Small';\r\n            case 1:\r\n                return 'Medium';\r\n            case 2:\r\n                return 'Large'\r\n        }\r\n    }\r\n\r\n    return (\r\n        <Layout>\r\n            <div className={css.container}>\r\n                <span className={css.heading}>\r\n                    Order in process\r\n                </span>\r\n                <div className={css.details}>\r\n                    <div>\r\n                        <span>Order ID</span>\r\n                        <span>{order._id}</span>\r\n                    </div>\r\n                    <div>\r\n                        <span>Customer Name</span>\r\n                        <span>{order.name}</span>\r\n                    </div>\r\n                    <div>\r\n                        <span>Phone</span>\r\n                        <span>{order.phone}</span>\r\n                    </div>\r\n                    <div>\r\n                        <span>Method</span>\r\n                        <span>{ order.method === 0 ? 'Payment on delievery' : 'Online payment' }</span>\r\n                    </div>\r\n                    <div>\r\n                        <span>Pizzas</span>\r\n                        <span>{ order.pizzas.map((pizza, id) => {\r\n                            return (\r\n                                <div key={id}>{pizza.name} {displayPizzaSize(pizza.size)} x{pizza.quantity}</div>\r\n                            )\r\n                        }) }</span>\r\n                    </div>\r\n                    <div>\r\n                        <span>Total</span>\r\n                        <span>${order.total}</span>\r\n                    </div>\r\n                </div>\r\n                <div className={css.statusContainer}>\r\n                    \r\n                    <div className={css.status}>\r\n                        <UilBill width={50} height={50}/>\r\n                        <span>Payment </span>\r\n                        { order.method === 0 ? \r\n                            <span className={css.pending}>On delivery</span> :\r\n                            <span className={css.completed}>Completed</span> }\r\n                    </div>\r\n\r\n                    <div className={css.status}>\r\n                        <Image src={Cooking} alt=\"\" width={50} height={50}/>\r\n                        <span>Cooking</span>\r\n                        { order.status === 1 ? \r\n                        <div className={css.spinner}>\r\n                            <Image src={Spinner}/>\r\n                        </div> : '' }\r\n                        { order.status > 1 ? \r\n                        <span className={css.completed}>Completed!</span> : '' }\r\n                    </div>\r\n\r\n                    <div className={css.status}>\r\n                        <Image src={Onway} alt=\"\" width={50} height={50}/>\r\n                        <span>On the way</span>\r\n                        { order.status === 2 ? \r\n                        <div className={css.spinner}>\r\n                            <Image src={Spinner}/>\r\n                        </div> : '' }\r\n                        { order.status > 2 ? \r\n                        <span className={css.completed}>Completed!</span> : '' }\r\n                    </div>\r\n\r\n                    <div className={css.status}>\r\n                        <UilBox width={50} height={50}/>\r\n                        <span>Delivered</span>\r\n                        { order.status === 3 ? \r\n                        <div className={css.spinner}>\r\n                            <Image src={Spinner}/>\r\n                        </div> : '' }\r\n                        { order.status > 3 ? \r\n                        <span className={css.completed}>Completed!</span> : '' }\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </Layout>\r\n    )\r\n}","C:\\Users\\mikol\\Documents\\Projekty\\nodejs\\pizza-shop\\pages\\pizza\\[slug].js",[],[],"C:\\Users\\mikol\\Documents\\Projekty\\nodejs\\pizza-shop\\pages\\success.js",[],[],"C:\\Users\\mikol\\Documents\\Projekty\\nodejs\\pizza-shop\\pages\\_app.js",[],[],"C:\\Users\\mikol\\Documents\\Projekty\\nodejs\\pizza-shop\\components\\Footer.jsx",[],[],"C:\\Users\\mikol\\Documents\\Projekty\\nodejs\\pizza-shop\\components\\Header.jsx",[],[],"C:\\Users\\mikol\\Documents\\Projekty\\nodejs\\pizza-shop\\components\\Hero.jsx",[],[],"C:\\Users\\mikol\\Documents\\Projekty\\nodejs\\pizza-shop\\components\\Layout.jsx",[],[],"C:\\Users\\mikol\\Documents\\Projekty\\nodejs\\pizza-shop\\components\\Menu.jsx",[],[],"C:\\Users\\mikol\\Documents\\Projekty\\nodejs\\pizza-shop\\components\\OrderModal.jsx",[],[],"C:\\Users\\mikol\\Documents\\Projekty\\nodejs\\pizza-shop\\components\\Services.jsx",[],[],"C:\\Users\\mikol\\Documents\\Projekty\\nodejs\\pizza-shop\\lib\\client.js",[],[],"C:\\Users\\mikol\\Documents\\Projekty\\nodejs\\pizza-shop\\lib\\orderHandler.js",[],[],{"ruleId":"91","severity":1,"message":"92","line":93,"column":29,"nodeType":"93","endLine":93,"endColumn":51},{"ruleId":"91","severity":1,"message":"92","line":104,"colum